{
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "plugins": [],
  "rules": {
    "no-use-before-define": ["error", { "functions": false }],
    "comma-dangle": ["error", "always-multiline"],
    "arrow-parens": 0,
    "consistent-return": 0
  },
  "globals": {
    "crypto": true,
    "fetch": true,
    "TEMPLATES": true
  }
}
